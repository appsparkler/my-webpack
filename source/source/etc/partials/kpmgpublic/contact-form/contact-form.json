{
    "hbs": {
        "contactForm": {
            "title": "Contact form",
            "shortTitle": "This is short title",
            "description": "This is a description for this form",
            "shortDescription": "This is short description",
            "required": "forms.required",
            "sorry" : "errorMessages.forms.sorry",
            "checkFollowingFields" : "errorMessages.forms.checkFollowingFields",
            "isCQ":false,
            "inquiry": {
                "label": "forms.inquiry",
                "errorMsg": "errorMessages.forms.inquiryRequired",
                "content": [
                    {
                        "name": "typeOne",
                        "value": "typeOne"
                    },
                    {
                        "name": "typeTwo",
                        "value": "typeTwo"
                    }
                ]
            },
            "locations": {
                "label": "forms.location",
                "helpMessage": "forms.messages.location",
                "errorMsg": "errorMessages.forms.locationRequired",
                "content": [
                    {
                        "name": "canada",
                        "value": "canada"
                    },
                    {
                        "name": "usa",
                        "value": "USA"
                    }
                ]
            },
            "name": {
                "label": "forms.name.short",
                "errorMsg": "errorMessages.forms.nameRequired"
            },
            "email": {
                "label": "forms.email",
                "helpMessage": "forms.messages.email",
                "errorMsg": "errorMessages.forms.emailRequired"
            },
            "phone": {
                "label": "forms.phone.short"
            },
            "organization": {
                "label": "forms.company.short"
            },
            "role": {
                "label": "forms.company.jobTitle"
            },
            "message": {
                "label": "forms.message",
                "errorMsg": "errorMessages.forms.messageRequired",
                "helpMessage": "forms.messages.message"
            },
            "captcha": {
                "label": "forms.captcha",
                "errorMsg": "errorMessages.forms.captchaRequired",
                "url" : "/etc/partials/kpmgpublic/contact-form/templates/captcha.captcha.png"
            },               
            "privacyPolicy": {
                "label": "forms.messages.privacyPolicy",
                "errorMsg": "errorMessages.forms.privacyPolicyRequired",
                "url": "#",
                "terms":"forms.messages.terms",
                "helpmessage" :"forms.messages.helpmessage"
            },
            "send": {
                "label": "forms.buttons.send",
                "url":"/kpmg/GenericContactFormServlet"
            },
            "refreshLabel":"forms.buttons.refresh"
        }
    }
}